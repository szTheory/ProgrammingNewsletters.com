    <div id="logo-text">
      <h1>Programming<br/>Newsletters<span class="accent">.com</span></h1>
    </div>
  </div>

  <ul id="nav-links">
    <% foreach my $category (@{$categories}) { %>
      <li class="<%= $category eq 'All' ? 'selected' : '' %>"><%= $category %></li>
    <% } %>
  </ul>

  <div class="entry-group">
    <% foreach my $entry_group (@{$grouped_entries}) { %>
      <h2 class="date topoff"><%= $entry_group->{date} %></h2>

      <ul class="entries">
      <% foreach my $entry (@{$entry_group->{entries}}) { %>
        <li class="entry" data-category="<%= $entry->{category} %>">
          <a href="<%= $entry->{url} %>" rel="nofollow" target="_blank"><%= $entry->{name} %></a>
        </li>
      <% } %>
      </ul>

    <% } %>
  </div>

  <footer>
    Â©<%= $year %> <%= $developer %> :: <a href="<%= $source_url %>">Source code</a>
  </footer>
</body>

</html>
